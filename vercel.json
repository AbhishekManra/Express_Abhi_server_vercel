{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/generate",
        "methods": ["POST", "OPTIONS"],
        "dest": "/index.js"
      },
      {
        "src": "/generatepost",
        "methods": ["POST", "OPTIONS"],
        "dest": "/index.js"
      },
      {
        "src": "/generatecomment",
        "methods": ["POST", "OPTIONS"],
        "dest": "/index.js"
      }
    ],
    "outputDirectory": "dist"
  }
  