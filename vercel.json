{
    "version": 2,
    "builds": [
      {
        "source": "./index.mjs",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/generate",
        "methods": ["POST", "OPTIONS"],
        "destination": "/index.mjs"
      },
      {
        "source": "/generatepost",
        "methods": ["POST", "OPTIONS"],
        "destination": "/index.mjs"
      },
      {
        "source": "/generatecomment",
        "methods": ["POST", "OPTIONS"],
        "destination": "/index.mjs"
      },
      {
        "source": "/generatemessage",
        "methods": ["POST", "OPTIONS"],
        "destination": "/index.mjs"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
  ],
    "outputDirectory": "dist"
  }
  